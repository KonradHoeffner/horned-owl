        <creator xml:lang="en">Phillip Lord</creator>
        <rdfs:label xml:lang="it">Pesce Ingredienti</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ncl.ac.uk/pizza#FourCheesePizza -->

    <owl:Class rdf:about="http://www.ncl.ac.uk/pizza#FourCheesePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ncl.ac.uk/pizza#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ncl.ac.uk/pizza#hasTopping"/>
                        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">4</owl:qualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.ncl.ac.uk/pizza#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <creator xml:lang="en">Phillip Lord</creator>
        <rdfs:label xml:lang="it">Pizza con Quattro Formaggio</rdfs:label>
    </owl:Class>
    


