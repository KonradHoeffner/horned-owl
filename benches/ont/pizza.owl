    <!-- http://www.ncl.ac.uk/pizza#DeepPanBase -->

    <owl:Class rdf:about="http://www.ncl.ac.uk/pizza#DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.ncl.ac.uk/pizza#PizzaBase"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ncl.ac.uk/pizza#hasCalorificContentValue"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">250</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.ncl.ac.uk/pizza#ThinAndCrispyBase"/>
        <creator xml:lang="en">Phillip Lord</creator>
        <rdfs:label xml:lang="pt">BaseEspessa</rdfs:label>
        <rdfs:label xml:lang="it">Disastro</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ncl.ac.uk/pizza#FishTopping -->

    <owl:Class rdf:about="http://www.ncl.ac.uk/pizza#FishTopping">
