    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ncl.ac.uk/pizza#hasCalorificContentValue -->

    <owl:DatatypeProperty rdf:about="http://www.ncl.ac.uk/pizza#hasCalorificContentValue">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <creator xml:lang="en">Phillip Lord</creator>
    </owl:DatatypeProperty>
    


