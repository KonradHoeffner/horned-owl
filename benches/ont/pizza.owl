        <owl:disjointWith rdf:resource="http://www.ncl.ac.uk/pizza#RosemaryTopping"/>
        <creator xml:lang="en">Phillip Lord</creator>
        <rdfs:label xml:lang="it">Cajun Spezia Ingredienti</rdfs:label>
    </owl:Class>
    


