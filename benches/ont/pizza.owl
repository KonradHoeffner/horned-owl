<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.ncl.ac.uk/pizza#"
     xml:base="http://www.ncl.ac.uk/pizza"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:piz="http://www.ncl.ac.uk/pizza#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:pattern="http://www.w3id.org/ontolink/pattern.owl#"
     >
    <owl:Ontology rdf:about="http://www.ncl.ac.uk/pizza">
        <rdfs:comment xml:lang="en">An example ontology modelled on the Pizza tutorial ontology from
Manchester University, written using the tawny-owl library</rdfs:comment>
        <rdfs:seeAlso xml:lang="en">Manchester Version</rdfs:seeAlso>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ncl.ac.uk/pizza#creator -->

    <owl:AnnotationProperty rdf:about="http://www.ncl.ac.uk/pizza#creator">
        <creator xml:lang="en">Phillip Lord</creator>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#comment"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.ncl.ac.uk/pizza#myOpinion -->

    <owl:AnnotationProperty rdf:about="http://www.ncl.ac.uk/pizza#myOpinion">
        <creator xml:lang="en">Phillip Lord</creator>
        <rdfs:comment xml:lang="en">Do I think this is a good pizza to eat?</rdfs:comment>
        <rdfs:label xml:lang="en">My Opinion</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#comment"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3id.org/ontolink/pattern.owl#facetvalue -->

    <owl:AnnotationProperty rdf:about="http://www.w3id.org/ontolink/pattern.owl#facetvalue"/>
    


    <!-- http://www.w3id.org/ontolink/pattern.owl#inpattern -->

    <owl:AnnotationProperty rdf:about="http://www.w3id.org/ontolink/pattern.owl#inpattern"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ncl.ac.uk/pizza#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.ncl.ac.uk/pizza#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.ncl.ac.uk/pizza#hasIngredient"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.ncl.ac.uk/pizza#Pizza"/>
        <rdfs:range rdf:resource="http://www.ncl.ac.uk/pizza#PizzaBase"/>
        <creator xml:lang="en">Phillip Lord</creator>
        <rdfs:label xml:lang="it">con Baso </rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ncl.ac.uk/pizza#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.ncl.ac.uk/pizza#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.ncl.ac.uk/pizza#isIngredientOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <creator xml:lang="en">Phillip Lord</creator>
        <rdfs:label xml:lang="it">con Ingredienti</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ncl.ac.uk/pizza#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.ncl.ac.uk/pizza#hasSpiciness">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.ncl.ac.uk/pizza#Spiciness"/>
        <creator xml:lang="en">Phillip Lord</creator>
        <rdfs:label xml:lang="it">con Piccantezza</rdfs:label>
        <pattern:inpattern rdf:nodeID="genid1"/>
    </owl:ObjectProperty>
    <rdf:Description rdf:nodeID="genid1"/>
    


    <!-- http://www.ncl.ac.uk/pizza#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.ncl.ac.uk/pizza#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.ncl.ac.uk/pizza#hasIngredient"/>
        <rdfs:domain rdf:resource="http://www.ncl.ac.uk/pizza#Pizza"/>
        <rdfs:range rdf:resource="http://www.ncl.ac.uk/pizza#PizzaTopping"/>
        <creator xml:lang="en">Phillip Lord</creator>
        <rdfs:label xml:lang="it">con Ingredienti</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ncl.ac.uk/pizza#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.ncl.ac.uk/pizza#isIngredientOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <creator xml:lang="en">Phillip Lord</creator>
        <rdfs:label xml:lang="it">Ã¨ Ingredienti di</rdfs:label>
    </owl:ObjectProperty>
    


