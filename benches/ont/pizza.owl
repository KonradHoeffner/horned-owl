        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ncl.ac.uk/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.ncl.ac.uk/pizza#HamTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ncl.ac.uk/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.ncl.ac.uk/pizza#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ncl.ac.uk/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.ncl.ac.uk/pizza#OliveTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ncl.ac.uk/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.ncl.ac.uk/pizza#PeperonataTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ncl.ac.uk/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.ncl.ac.uk/pizza#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ncl.ac.uk/pizza#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ncl.ac.uk/pizza#AnchoviesTopping"/>
                            <rdf:Description rdf:about="http://www.ncl.ac.uk/pizza#CaperTopping"/>
                            <rdf:Description rdf:about="http://www.ncl.ac.uk/pizza#HamTopping"/>
                            <rdf:Description rdf:about="http://www.ncl.ac.uk/pizza#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.ncl.ac.uk/pizza#OliveTopping"/>
                            <rdf:Description rdf:about="http://www.ncl.ac.uk/pizza#PeperonataTopping"/>
                            <rdf:Description rdf:about="http://www.ncl.ac.uk/pizza#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <creator xml:lang="en">Phillip Lord</creator>
        <rdfs:label xml:lang="it">CapricciosaPizza</rdfs:label>
    </owl:Class>
    


