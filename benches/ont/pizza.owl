    </owl:Class>
    


    <!-- http://www.ncl.ac.uk/pizza#CajunPizza -->

    <owl:Class rdf:about="http://www.ncl.ac.uk/pizza#CajunPizza">
        <rdfs:subClassOf rdf:resource="http://www.ncl.ac.uk/pizza#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ncl.ac.uk/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.ncl.ac.uk/pizza#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ncl.ac.uk/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.ncl.ac.uk/pizza#OnionTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ncl.ac.uk/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.ncl.ac.uk/pizza#PeperonataTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ncl.ac.uk/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.ncl.ac.uk/pizza#PrawnsTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ncl.ac.uk/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.ncl.ac.uk/pizza#TobascoPepperSauce"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ncl.ac.uk/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.ncl.ac.uk/pizza#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ncl.ac.uk/pizza#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.ncl.ac.uk/pizza#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.ncl.ac.uk/pizza#OnionTopping"/>
                            <rdf:Description rdf:about="http://www.ncl.ac.uk/pizza#PeperonataTopping"/>
                            <rdf:Description rdf:about="http://www.ncl.ac.uk/pizza#PrawnsTopping"/>
                            <rdf:Description rdf:about="http://www.ncl.ac.uk/pizza#TobascoPepperSauce"/>
                            <rdf:Description rdf:about="http://www.ncl.ac.uk/pizza#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <creator xml:lang="en">Phillip Lord</creator>
        <rdfs:label xml:lang="it">Cajun Pizza</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ncl.ac.uk/pizza#CajunSpiceTopping -->

    <owl:Class rdf:about="http://www.ncl.ac.uk/pizza#CajunSpiceTopping">
