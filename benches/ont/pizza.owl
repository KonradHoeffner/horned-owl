        <creator xml:lang="en">Phillip Lord</creator>
        <rdfs:label xml:lang="it">Formaggio Ingredienti</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ncl.ac.uk/pizza#CheesyPizza -->

    <owl:Class rdf:about="http://www.ncl.ac.uk/pizza#CheesyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ncl.ac.uk/pizza#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.ncl.ac.uk/pizza#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.ncl.ac.uk/pizza#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <creator xml:lang="en">Phillip Lord</creator>
        <rdfs:label xml:lang="it">Pizza con Formaggio</rdfs:label>
    </owl:Class>
    


